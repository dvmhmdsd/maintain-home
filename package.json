{
  "name": "maintain-home",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build:watch": "tsc -w",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "dev-start": "npm run build && concurrently \"nodemon\" \"cd dashboard/ && ng serve\"",
    "back-dev-start": "concurrently 'npm run build:watch' 'cd dist/ && nodemon' && cd .."
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
      "npm run prebuild",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "cloudinary": "^1.21.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.9",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "pm2": "^4.2.3",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
